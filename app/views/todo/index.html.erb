<div class="index">
<h1 id="mytitle">My To-Do List</h1>
<ul>
<% @todos.each do |todo| %>
   <li class="list-item">
    <input type="checkbox">
   <a href="/todo/show/<%= todo.id %>" ><%= todo.task_name %></a>
    <a href="/todo/show/<%= todo.id %>" ><%= todo.due_date %></a>
    </li>
<% end %>
</ul>
<a href="/todo/new" >Create New To-Do</a>
</div>